# Kafka configuration
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC=stock-market-data
KAFKA_CONSUMER_GROUP=stock-market-group

# Producer settings
PRODUCER_INTERVAL=1  # seconds between messages
PRODUCER_COUNT=100   # total messages to produce (optional)

# Consumer settings
OUTPUT_DIR=../data

# Spark settings
INPUT_PATH=../data
OUTPUT_PATH=../output

# Data warehouse connection
WAREHOUSE_JDBC_URL=jdbc:postgresql://localhost:5433/datawarehouse
WAREHOUSE_USER=datauser
WAREHOUSE_PASSWORD=datapass

# Logging
LOG_LEVEL=INFO 